{
    "codehighlight":{
        "window": {
            "id": "toolkit-block-cdht",
            "class": "toolkit-block",
            "style": ""
        },
        "window-head": {
            "title": "CodeHighlight - ToolkitSwiss"
            ,"style": ""
            ,"class": ""
        },
        "window-content":{
            "contents":["highlighter-ct"],
            "data-contents":{
                "highlighter-ct":{
                    "class": "noback",
                    "style": "width:100%;height:100%;padding:0px;box-sizing:border-box;text-align:center;",
                    "html":[
                        "<div id='data-elements' class='tkcont-child noback'>",
                        "  <div class='title' style='height:22px !important;padding:0px !important;margin:0px !important;'>",
                        "    Code",
                        "  </div>",
                        "  <textarea style='width:100%;height:150px;font-size:12px;' class='code-to-highlight'>",
                        "  </textarea>",
                        "</div>",

                        "<div id='data-elements' class='tkcont-child noback'>",
                        "  <div class='title' style='padding:0px !important;margin:0px !important;'>",
                        "    <select class='lang-to'>",
                        "      <option value='-1'>Select Language</option>",
                        "      <option value='C'>Language C</option>",
                        "    </select>",
                        "  </div>",
                        "  <textarea style='width:100%;height:150px;overflow-y:scroll;font-size:12px;' class='code-highlighted'>",
                        "  </textarea>",
                        "</div>",

                        "<div id='cdh-options' style='text-align:center;width:100%;'>",
                        "  <div class='error-raise' style='width:100%;height:20px;'></div>",
                        "  <br/>",
                        "  <button class='btnHighlight' style='padding:1px;'>Highlight!</button>",
                        "</div>"
                    ]
                }
            }
        },
        "window-footer":{
            "style": "visibility:hidden;",
            "footer": ""
        }
    }    
}